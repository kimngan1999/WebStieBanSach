    <script src="~/Content/TemplateAmin/js/excanvas.js"></script>
    <script src="~/Content/TemplateAmin/js/jquery.flot.js"></script>
    <script src="~/Content/TemplateAmin/js/jquery.flot.pie.js"></script>
    <script src="~/Content/TemplateAmin/js/jquery.flot.stack.js"></script>
    <script src="~/Content/TemplateAmin/js/jquery.flot.resize.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.chosen.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.uniform.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.cleditor.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.noty.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.elfinder.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.raty.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.iphone.toggle.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.gritter.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.imagesloaded.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.masonry.min.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.knob.modified.js"></script>

    <script src="~/Content/TemplateAmin/js/jquery.sparkline.min.js"></script>

    <script src="~/Content/TemplateAmin/js/counter.js"></script>

    <script src="~/Content/TemplateAmin/js/retina.js"></script>

    <script src="~/Content/TemplateAmin/js/custom.js"></script>
    <!-- end: JavaScript-->

</body>
